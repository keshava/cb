{
  "$schema": "http://json-schema.org/schema#",
  "id": "https://uri.etsi.org/ngsi-ld/schema/Notification.json",
  "title": "NGSI-LD Notification",
  "description": "NGSI-LD Notification",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uri"
    },
    "type": {
      "type": "string",
      "const": "Notification"
    },
    "subscriptionId": {
      "type": "string",
      "format": "uri"
    },
    "@context": {
      "$ref": "https://forge.etsi.org/gitlab/NGSI-LD/NGSI-LD/raw/master/schema/common.json#/definitions/LdContext"
    },
    "notifiedAt": {
      "type": "string",
      "format": "date-time"
    },
    "data": {
      "type": "array",
      "items": {
        "$ref": "https://forge.etsi.org/gitlab/NGSI-LD/NGSI-LD/raw/master/schema/Entity.json#/definitions/Entity"
      },
      "minItems": 1
    }
  },
  "required": [
    "id",
    "type",
    "notifiedAt",
    "data"
  ]
}
